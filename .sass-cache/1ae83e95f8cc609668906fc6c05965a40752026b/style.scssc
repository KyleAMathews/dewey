    &:hover {
      background: $dark;
      color: white; } } }

.create-new-group {
  font-size: 110%; }

/* Node Styles */

.node {
  border-bottom: 1px solid $light2;
  padding-bottom: 1em; }

.meta {
  margin-bottom: 1em; }

.meta-info {
  margin-top: 1em; }

h2.node-title {
  margin-bottom: 0;
  line-height: 25px;
  a {
    text-decoration: none;
    color: $dark;
    &:hover {
      text-decoration: underline; } } }

.node-submitted {
  display: table-cell;
  vertical-align: middle;
  color: $light2;
  a {
    color: $light2;
    text-decoration: none;
    &:hover {
      text-decoration: underline; } } }

.conversation-bubble-container {
  margin-top: -70px;
  a {
    text-decoration: none;
    color: $light2; } }

.conversation-teaser a:hover {
  text-decoration: underline; }

.conversation-bubble {
  border: 1px solid black;
  padding: 1em;
  line-height: 1.2em;
  position: relative;
  margin-bottom: 30px;
  overflow: hidden;
  img {
    position: absolute;
    left: 86px;
