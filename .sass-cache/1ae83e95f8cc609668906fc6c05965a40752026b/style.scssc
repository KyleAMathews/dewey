    -webkit-border-radius: 10px;
    border-radus: 10px;
    &:hover {
      color: white;
      background-color: $light2;
      border: none;
      padding: 4px 9px;
      -moz-border-radius: 10px;
      -webkit-border-radius: 10px;
      border-radus: 10px; } } }

/**
 * Module: DIFF =======================================================
 */

div {
  &.diff-inline-controls {
    span.diff-inline-legend {
      float: left; }
    float: right;
    background-color: inherit; }
  &.block-diff {
    div.block-content {
      max-height: 200px;
      overflow: auto; }
    table.diff-inline-revisions tr {
      color: $grey60;
      background: transparent;
      border-top: 1px solid $grey93;
      &:hover {
        color: $grey60;
        background: transparent;
        border-top: 1px solid $grey93; }
      &.active {
        color: $grey20; } } } }

span {
  &.diff-deleted {
    color: $grey80; }
  &.diff-added, &.diff-changed {
    padding-top: 5px;
    padding-bottom: 5px;
    background: #ffc;
    border-bottom: 1px solid #fe9;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px; }
  &.diff-added {
    background: #cfc;
    border-color: #9c9; } }

.blank-slate {
  border: 1px solid $blank_slate;
  color: #33A333;
  margin-top: 1em;
  padding: 10px;
  display: block;
  text-align: center;
  background-color: window; }

/* Table overrides */

tr {
  &.odd .form-item, &.even .form-item {
    max-width: 100%; } }

.ahah-progress {
  height: 26px;
