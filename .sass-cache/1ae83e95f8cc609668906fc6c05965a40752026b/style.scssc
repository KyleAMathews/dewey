/** Pagers **/

.item-list .pager li, .pager li a {
  background: $dark;
  color: white; }

.item-list .pager li.pager-current {
  background: white;
  color: $dark;
  border: 1px solid $dark; }

#space-title h2 {
  font-family: helvatica, arial, sans-serif;
  font-weight: bolder;
  font-size: 300%;
  margin: 0px 0 15px 0;
  line-height: 30px;
  a {
    color: black;
    text-decoration: none; } }

#ayour-groups-dropdown {
  background: white;
  position: absolute;
  left: 24px;
  top: 23px;
  display: none;
  z-index: 101;
  padding: 15px;
  border: 5px solid $dark;
  p {
    margin: 7px 0 0 0; }
  h2 {
    margin-bottom: 0; }
  hr {
    margin-top: 0; }
