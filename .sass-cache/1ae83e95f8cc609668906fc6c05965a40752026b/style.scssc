/* Messages */
.messages {
  background-color: white;
  border: 1px solid #b8d3e5; }

div {
  &.status {
    color: #33a333;
    border-color: $blank_slate; }
  &.error {
    color: #a30000;
    background-color: #FFCCCC; } }

tr.error {
