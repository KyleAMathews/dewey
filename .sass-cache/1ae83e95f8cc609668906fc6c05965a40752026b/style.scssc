/* Messages */
.messages {
  background-color: white;
  border: 1px solid #b8d3e5; }

div {
  &.status {
    color: #33a333;
    border-color: $blank_slate; }
  &.error {
    color: #a30000;
    background-color: #FFCCCC; } }

tr.error {
  color: #a30000;
  background-color: #FFCCCC; }

.form-item {
  input.error, textarea.error {
    border: 1px solid #c52020;
    color: #363636; } }

/* Vertical Tabs are too wide */

div.vertical-tabs {
